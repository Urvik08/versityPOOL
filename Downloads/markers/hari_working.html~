function initMap() {
  var myLatLng = {lat: 28, lng: -83.25};
console.log("jj");

  var map = new google.maps.Map(document.getElementById('map'), {
    zoom: 4,
    center: myLatLng
  });

  var marker = new google.maps.Marker({
    position: myLatLng,
    map: map,
    title: 'Hello World!'
  });
}

   
  
